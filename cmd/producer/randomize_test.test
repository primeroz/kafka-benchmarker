package main

import (
	"fmt"
	"math"
	"math/rand"
)

func main() {
	var topicId int
	topicRangeStart := 0
	topicRangeEnd := 24999
	dict := make(map[int]int)

	for m := 0; m < 1000000; m++ {
		//topicId = rand.Intn(topicRangeEnd-topicRangeStart) + (topicRangeStart)
		topicId = int(math.Min((rand.ExpFloat64()/30.0)*float64(topicRangeEnd-topicRangeStart)+float64(topicRangeStart), float64(topicRangeEnd)))
		dict[topicId] = dict[topicId] + 1
	}
	fmt.Println(dict)

}
